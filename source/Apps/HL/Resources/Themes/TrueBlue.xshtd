    <Color name="Image"          foreground="#FF40BFFF" exampleText="[text][http://example.com/test.png]"/>
    <Color name="LineBreak"      foreground="#FF222222" exampleText="end of line      \r\n2nd line   "/>
  </SyntaxDefinition>

  <SyntaxDefinition name = "Pascal" extensions = ".pas">
