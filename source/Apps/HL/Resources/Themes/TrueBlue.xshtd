  <SyntaxDefinition name="Patch" extensions=".patch;.diff">
    <Color name="AddedText"     foreground="#FFFFFF00" exampleText="+added"/>
    <Color name="RemovedText"   foreground="#FFFF6060" exampleText="-removed"/>
