    <Color name="SelectionStatements" foreground="#FFFFFF40" fontWeight="bold" exampleText="if (true) { } else { }" />
    <Color name="IterationStatements" foreground="#FFDFDF40" fontWeight="bold" exampleText="while (true) { }" />
